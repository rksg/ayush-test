{
  "name": "Extract RC Cookie",
  "version": "1.1",
  "description": "Copy RC Cookies to localhost domain",
  "icons": {
    "128": "concept-128x128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "nativeMessaging",
    "notifications" ,
    "https://*.ruckuswireless.com/",
    "http://*.ruckuswireless.com/",
    "https://ruckus.cloud/",
    "http://ruckus.cloud/",
    "http://127.0.0.1/",
    "http://localhost/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "concept-128x128.png",
    "default_title": "Click the icon to copy cookies from configured domain to localhost. Default is set to devalto. Domain can be changed in options"
  },
  "manifest_version": 2
}
