import { LteBandRegionEnum } from '../constants'

export class AvailableLteBands {
  band3G?: string[]

  band4G?: string[]

  region: LteBandRegionEnum

  countryCodes?: string[]

  constructor () {
    this.band3G = []

    this.band4G = []

    this.region = LteBandRegionEnum.DOMAIN_1

    this.countryCodes = []
  }
}
