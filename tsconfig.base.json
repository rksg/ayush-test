{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom", "dom.iterable", "ES2021.String", "ES2022.Array"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@acx-ui/analytics/components": [
        "libs/analytics/components/src/index.ts"
      ],
      "@acx-ui/analytics/services": ["libs/analytics/services/src/index.ts"],
      "@acx-ui/analytics/utils": ["libs/analytics/utils/src/index.ts"],
      "@acx-ui/components": ["libs/common/components/src/index.ts"],
      "@acx-ui/config": ["libs/common/config/src/index.ts"],
      "@acx-ui/feature-toggle": ["libs/common/feature-toggle/src/index.ts"],
      "@acx-ui/formatter": ["libs/common/formatter/src/index.ts"],
      "@acx-ui/icons": ["libs/common/icons/src/index.ts"],
      "@acx-ui/main/components": ["libs/main/components/src/index.ts"],
      "@acx-ui/msp/components": ["libs/rc/msp/components/src/index.ts"],
      "@acx-ui/msp/services": ["libs/rc/msp/services/src/index.ts"],
      "@acx-ui/msp/utils": ["libs/rc/msp/utils/src/index.ts"],
      "@acx-ui/rc/components": ["libs/rc/shared/components/src/index.ts"],
      "@acx-ui/rc/services": ["libs/rc/shared/services/src/index.ts"],
      "@acx-ui/rc/utils": ["libs/rc/shared/utils/src/index.ts"],
      "@acx-ui/react-router-dom": ["libs/common/react-router-dom/src/index.ts"],
      "@acx-ui/reports/components": ["libs/reports/components/src/index.ts"],
      "@acx-ui/reports/services": ["libs/reports/services/src/index.ts"],
      "@acx-ui/reports/utils": ["libs/reports/utils/src/index.ts"],
      "@acx-ui/store": ["libs/common/store/src/index.ts"],
      "@acx-ui/test-utils": ["libs/common/test-utils/src/index.ts"],
      "@acx-ui/theme": ["libs/common/components/src/theme/index.less"],
      "@acx-ui/types": ["libs/common/types/src/index.ts"],
      "@acx-ui/user": ["libs/common/user/src/index.ts"],
      "@acx-ui/utils": ["libs/common/utils/src/index.ts"],
      "@rc/Routes": ["apps/rc/src/Routes.tsx"],
      "@analytics/Routes": ["apps/analytics/src/Routes.tsx"],
      "@msp/Routes": ["apps/msp/src/Routes.tsx"],
      "@reports/Routes": ["apps/reports/src/Routes.tsx"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
